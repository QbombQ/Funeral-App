{
  "expo": {
    "id": "ad25d90a-227d-4428-b8fb-06ca420cace5",
    "owner": "nandes",
    "name": "FuneralApp",
    "slug": "funeralapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.anonymous.FuneralApp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "supportsAppleSignIn": true,
      "bundleIdentifier": "com.anonymous.FuneralApp",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.361140166748-bmj9mrcq3ffj1coqvhd0lv9l0hht4jcq"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "NOTIFICATIONS",
        "INTERNET",
        "NOTIFICATIONS"
      ],
      "package": "com.anonymous.FuneralApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps._some_id_here_"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}